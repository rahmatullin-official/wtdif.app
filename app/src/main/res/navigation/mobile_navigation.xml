<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/homeScreenFragment2">

    <fragment
        android:id="@+id/planeScreenFragment2"
        android:name="space.rahmatullin.firstapp.PlaneScreenFragment"
        android:label="fragment_plane_screen"
        tools:layout="@layout/fragment_plane_screen" >
        <action
            android:id="@+id/action_planeScreenFragment2_to_schoolScreenFragment2"
            app:destination="@id/schoolScreenFragment2" />
    </fragment>
    <fragment
        android:id="@+id/schoolScreenFragment2"
        android:name="space.rahmatullin.firstapp.SchoolScreenFragment"
        android:label="fragment_school_screen"
        tools:layout="@layout/fragment_school_screen" />
    <fragment
        android:id="@+id/homeScreenFragment2"
        android:name="space.rahmatullin.firstapp.HomeScreenFragment"
        android:label="fragment_home_screen"
        tools:layout="@layout/fragment_home_screen" >
        <action
            android:id="@+id/action_homeScreenFragment2_to_planeScreenFragment2"
            app:destination="@id/planeScreenFragment2" />
    </fragment>
</navigation>